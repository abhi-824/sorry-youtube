{
    "name": "YouTube Playlist downlaoder",
    "description": "Download your liked songs by click of a button",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["activeTab","tabs"],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts": [
        {
          "matches": ["*://*.music.youtube.com/*"],
          "js": ["index.js"]
        }
    ] 
    
  }